@page
@using web_NIN.Models
@model web_NIN.Pages.OutboundModel
@{
}
@{
    Layout = "~/Pages/Shared/_Outbound.cshtml";
}

<div class="card tt">

    <div class="card-body">
        @*<button type="button" class="btn btn-outline-warning" onclick="call_action()">OUTBOUND</button>*@
        <button class="btn btn-primary" onclick="call_action()">OUTBOUND</button>
    </div>

</div>
@* ---------------------------------------สถานะการติดต่อ----------------------------------------------- *@
<div class="card tt">
    <div class="card-header">
        <h3 class="card-title">สถานะการติดต่อ</h3>

    </div>
    <p id="tel" hidden>@ViewData["tel"]</p>
    <p id="mom_id" hidden>@ViewData["mom_id"]</p>
    <p id="agent" hidden>@ViewData["agent"]</p>
    <div class="card-body">


        <div class="form-group">
            @*<label for="exampleSelectBorder">สถานะการติดต่อ</label>*@
            <select class="custom-select form-control-border tk_input" id="statuscall" onchange="status_call()">
                <option value="select">เลือกสถานะการติดต่อ</option>
                @foreach (var std in (IEnumerable<Status_call_model>)ViewData["status_call"])
                {
                <option value="@std.eng_status">@std.thai_status</option>
                }

            </select>
        </div>
    </div>

</div>
@* --------------------------------------------------------------------------------------------------- *@


@* ------------------------------นัดเวลาติดต่อ------------------------------------------------------------ *@
<div class="card tt" id="call_agent" style="display: none;">
    <div class="card-header">
        <h3 class="card-title">ต้องการนัดเวลาติดต่อ</h3>
    </div>


    <div class="card-body">
        <div class="form-group">
            @*<label for="exampleSelectBorder">สถานะการติดต่อ</label>*@
            <div class="form-group col-xs-3">
                <label>
                    ต้องการนัดเวลาติดต่อ

                </label>
                <div class="input-group date" id="reservationdatetime" data-target-input="nearest" hidden>
                    <input type="text" class="form-control datetimepicker-input tk_input" data-target="#reservationdatetime" id="next_time" onclick="save_log(3)" />
                    <div class="input-group-append" data-target="#reservationdatetime" data-toggle="datetimepicker">
                        <div class="input-group-text"><i class="fa fa-calendar"></i></div>
                    </div>
                </div>

                <br />

                <div class="input-group date">
                    <div id="picker"></div>
                    <input type="text" id="result_date" value="" />
                </div>
                @*<div style="width: 250px; margin: 50px auto;">
                        <div id="picker-no-time"></div>
                        <input type="text" id="result2" value="" />
                    </div>*@
                @*<button type="button" class="btn btn-secondary" onclick="save_log(2)">ไม่ให้ติดต่อกลับ</button>*@
            </div>
        </div>
    </div>

</div>


@* --------------------------------------------------------------------------------------------------- *@
<div class="card tt">
    <div class="card-header">
        <h3 class="card-title">ประวัติลูกค้า</h3>
    </div>
    <div class="card-body">

        <!-- /.card-header -->
        <!-- form start -->


        <div class="form-group col-xs-3">
            <label for="exampleInputEmail1">ชื่อ-นามสกุลคุณแม่</label>
            <input type="text" class="form-control tk_input" id="mom_name" value="@ViewData["customer_name"]">
        </div>
        <div class="form-group col-xs-3">
            <label for="exampleInputPassword1">
                กำหนดคลอดที่คาดการณ์ไว้
            </label>
            <input type="text" class="form-control tk_input" id="time_baby" value="@ViewData["Child_birthdate"]">
        </div>
        <div class="form-group col-xs-3">
            <label for="exampleInputPassword1">
                อายุบุตร
            </label>
            <input type="text" class="form-control tk_input" id="baby_age" value="@ViewData["Child_age"]">
            @{
                var check = ViewData["Child_age"];
                if (string.IsNullOrEmpty(check.ToString()))
                {
                <br />
                <button type="button" class="btn btn-info" onclick="age_check()">คำนวนอายุ</button>
                }
            }
        </div>





    </div>
</div>

